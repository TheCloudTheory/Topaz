{
  "servers": {
    "TopazMCP": {
      "type": "stdio",
      "command": "dotnet",
      "args": [
        "run",
        "--project",
        "Topaz.MCP/Topaz.MCP.csproj"
      ]
    },
    "TopazMCPDocker": {
      "type": "stdio",
      "command": "docker",
      "args": [
        "run",
        "-i",
        "topaz/mcp"
      ]
    }
  }
}